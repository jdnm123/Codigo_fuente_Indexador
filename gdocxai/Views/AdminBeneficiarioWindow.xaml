<Window
    x:Class="Indexai.AdminBeneficiarioWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:Views="clr-namespace:Indexai.Views"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="Edición de beneficiarios"
    Width="750.4"
    Height="667.6"
    MinHeight="650"
    PreviewLostKeyboardFocus="Window_PreviewLostKeyboardFocus"
    WindowStartupLocation="CenterScreen"
    mc:Ignorable="d">
    <Grid x:Name="beneficiariosConatiner">
        <Views:BeneficiariosAdmin
            x:Name="viewBeneficiarios"
            Width="{Binding ElementName=beneficiariosConatiner, Path=ActualWidth}"
            Height="{Binding ElementName=beneficiariosConatiner, Path=ActualHeight}" />
    </Grid>
</Window>
